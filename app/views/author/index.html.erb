<h3><%=current_user.last_name%>さんの<%=@category_name.category_name%>のテストの結果</h3>
<% if @questions %>
  <table class="table table-striped">
    <thead>
      <tr>
        <th class="span1">問題ID</th>
        <th class="span8">問題</th>
        <th class="span1">問題ランク</th>
        <th class="span1">正答率</th>
      <tr>
    </thead>
    <tbody>
      <% @questions.each do |question| %>
        <tr>
          <td><%= question.id %></td>
          <td><%= question.question %></td>
          <td><%= question.rank %></td>
          <td><%= Question.correct_rate(question.id) %>％</td>
        </tr>
      <% end %>
    </tbody>
  </table>
<% end %>

